<!DOCTYPE html>
<html>
<head>
  <title>Scheduler</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
<!-- header -->
<%= render 'shared/header' %>
<section class="content">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
              <% flash.each do |message| %>
                  <%= message %>
              <% end %>
            </div>
        </div>

        <div class="row">
           <div class="col-md-12">
             <%= yield %>
           </div>
        </div>

    </div>
</section>
<!-- footer -->
<%= render 'shared/footer' %>
</body>
</html>
